<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <table cellspacing="1" cellpadding="15" align="center" border="1">
        <thead>
            <tr>
                <th>No.</th>
                <th>GRID</th>
                <th>Name</th>
                <th>Age</th>
                <th>Gender</th>
                <th>Email</th>
                <th>Course</th>
            </tr>
        </thead>
        <tbody>
            <% students.forEach((student ,index) =>{ %>
             <tr>
                <td><%= index +1 %></td>
                <td><%= student.grid %></td>
                <td><%= student.name %></td>
                <td><%= student.age %></td>
                <td><%= student.gender %></td>
                <td><%= student.email %></td>
                <td><%= student.course %></td>
             </tr>    
            <% })%>
        </tbody>
    </table>
</body>
</html>